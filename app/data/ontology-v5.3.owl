    <!-- http://www.semanticweb.org/nkb/ontologies/2013/6/impact-investment#hasProduct -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nkb/ontologies/2013/6/impact-investment#hasProduct">
