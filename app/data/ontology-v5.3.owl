        <verbose_name xml:lang="en">Fundraising round</verbose_name>
        <verbose_name_plural xml:lang="en">Fundraising rounds</verbose_name_plural>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


