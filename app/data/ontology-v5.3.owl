        <verbose_name xml:lang="en">Product</verbose_name>
        <verbose_name_plural xml:lang="en">Products</verbose_name_plural>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/nkb/ontologies/2013/6/impact-investment#hasRaised -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/nkb/ontologies/2013/6/impact-investment#hasRaised">
